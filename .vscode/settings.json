{
    "cSpell.language": ",pt,pt_BR",
    "spellright.language": [
        "pt_BR"
    ],
    "spellright.documentTypes": [
        "markdown",
        "plaintext",
        "latex"
    ],
    "latex.build.onSave": true,
    "latex.build.outputDirectory": "./dist/",
    "latex.build.args": [
        "-output-directory=./dist/",
        "-pdf",
        "-interaction=nonstopmode",
        "-synctex=1",
        "%f"
    ],
    "languageToolLinter.languageTool.motherTongue": "pt-BR",
    "languageToolLinter.languageTool.language": "pt-BR",
    "ltex.additionalRules.motherTongue": "pt-BR",
    "ltex.language": "pt-BR",
}